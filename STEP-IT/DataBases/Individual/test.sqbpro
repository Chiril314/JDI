	&quot;log_id&quot; INTEGER PRIMARY KEY AUTOINCREMENT, 
	&quot;actiune&quot; TEXT,
	&quot;student_id&quot; INTEGER, 
	&quot;grupa_id&quot; INTEGER NOT NULL,
	&quot;nume&quot; VARCHAR(50) NOT NULL,
	&quot;data_nasterii&quot; DATE NOT NULL, 
	&quot;IDNP&quot; VARCHAR(12) NOT NULL, 
	&quot;gen&quot; INTEGER NOT NULL
);

CREATE TRIGGER student_insert
	AFTER INSERT ON student
	FOR EACH ROW 
BEGIN
	INSERT INTO student_log(actiune, student_id, grupa_id, nume, data_nasterii, IDNP, gen)
	VALUES(
		&quot;INSERT&quot;,
		NEW.student_id,
		NEW.grupa_id, 
		NEW.nume || ' ' || NEW.prenume, 
		NEW.data_nasterii, 
		NEW.IDNP,
		NEW.gen);
END;

CREATE TRIGGER student_update
	AFTER UPDATE OF grupa_id ON student
	FOR EACH ROW
BEGIN
	INSERT INTO student_log(actiune, student_id, grupa_id, nume, data_nasterii, IDNP, gen)
	VALUES(
		&quot;UPDATE&quot;,
		OLD.student_id,
		OLD.grupa_id || &quot; -&gt; &quot; || NEW.grupa_id,
		OLD.nume || ' ' || OLD.prenume,
		OLD.data_nasterii,
		OLD.IDNP,
		OLD.gen);
END;

CREATE TRIGGER student_delete
	AFTER DELETE ON student
	FOR EACH ROW
BEGIN
	INSERT INTO student_log(actiune, student_id, grupa_id, nume, data_nasterii, IDNP, gen)
	VALUES(
		&quot;DELETE&quot;,
		OLD.student_id,
		OLD.grupa_id,
		OLD.nume || ' ' || OLD.prenume,
		OLD.data_nasterii,
		OLD.IDNP,
		OLD.gen);
END;

